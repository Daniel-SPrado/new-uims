xt-field>\n            </v-col>\n            <v-col cols=\"12\" sm=\"4\" md=\"3\">\n            <v-text-field\n                label=\"Number\"\n                v-model=\"service.number\"\n                disabled\n            ></v-text-field>\n            </v-col>\n            <v-col cols=\"12\" sm=\"4\" md=\"3\">\n            <v-text-field\n                label=\"Name\"\n                v-model=\"service.name\"\n                disabled\n            ></v-text-field>\n            </v-col>\n            <v-col cols=\"12\" sm=\"2\" md=\"3\">\n            <v-text-field\n                label=\"Parameter\"\n                v-model=\"service.parameter\"\n                disabled\n            ></v-text-field>\n            </v-col>\n            <v-col cols=\"12\" sm=\"2\" md=\"3\">\n            <v-text-field\n                label=\"Unit\"\n                v-model=\"service.unit\"\n                disabled\n            ></v-text-field>\n            </v-col>\n            <v-col cols=\"12\" sm=\"4\" md=\"3\">\n            <v-text-field\n                label=\"IP\"\n                v-model=\"service.ip\"\n            ></v-text-field>\n            </v-col>\n            <v-col cols=\"12\" sm=\"1\" md=\"1\">\n              <v-btn @click=\"editService()\">\n                enviar\n              </v-btn>\n            </v-col>\n        </v-row>\n        <v-divider></v-divider>\n        </div>\n        <v-row>\n          <v-col v-show=\"visibleDelete\" cols=\"12\" sm=\"12\" md=\"12\" class=\"toCenter\">\n              <v-btn color=\"red--text\" @click=\"deleteService()\">\n                  Delete Service\n              </v-btn>\n            </v-col>\n        </v-row>\n      \n\n        \n        <div v-show=\"visibleCommand\">\n        <v-card-title>\n            Add command\n        </v-card-title>\n            <v-row>\n              <v-col cols=\"12\" sm=\"4\" md=\"4\">\n                <v-text-field\n                    label=\"ip\"\n                    v-model=\"ip\"\n                ></v-text-field>\n              </v-col>\n              <v-col cols=\"12\" sm=\"4\" md=\"4\">\n                <v-text-field\n                    label=\"params\"\n                    v-model=\"params\"\n                ></v-text-field>\n              </v-col>\n              <v-col cols=\"12\" sm=\"4\" md=\"4\">\n                <v-text-field\n                    label=\"Button Name\"\n                    v-model=\"btnName\"\n                ></v-text-field>\n              </v-col>\n              <v-col cols=\"12\" sm=\"12\" md=\"12\" class=\"toCenter\">\n                <v-btn color=\"green--text\" @click=\"createCommand()\">\n                    Create Command\n                </v-btn>\n              </v-col>\n            </v-row>\n          </div>\n        \n        <v-divider></v-divider>\n        <v-card-title>\n            All Commands\n        </v-card-title>\n            <v-row>\n              <table>\n                  <tr>\n                    <th>Name</th>\n                    <th>IP</th>\n                    <th>Params</th>\n                    <th>Remove</th>\n                </tr>\n                <tr v-for=\"item in commands\" :key=\"item.mac\"> \n                    <td class=\"toCenter\">{{item.name}}</td>\n                    <td class=\"toCenter\">{{item.ip}}</td>\n                    <td class=\"toCenter\">{{item.params}}</td>\n                    <td class=\"tocenter\"><v-btn text @click=\"deleteCommand(service, item)\"> <v-icon>mdi-close</v-icon> </v-btn></td>\n                </tr>\n              </table>\n              \n              \n            </v-row>\n        </v-card-text>\n        <v-divider></v-divider>\n        <v-card-actions>\n          <v-btn\n            color=\"red\"\n            text\n            @click=\"visibleDelete = !visibleDelete\"\n          >\n            Delete Service\n          </v-btn>\n          <v-btn\n            color=\"green\"\n            text\n            @click=\"visibleCommand = !visibleCommand\"\n          >\n            Add command\n          </v-btn>\n          <v-spacer></v-spacer>\n          <v-btn\n            color=\"red\"\n            text\n            @click=\"dialog = false\"\n          >\n            Close\n          </v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </div>\n</template>\n\n<script>\nimport { Group, Command } from \"../../function/index.js\";\n\nlet novoGrupo = new Group();\nlet novoComando = new Command();\n\nexport default {\n  props: [ \"service\", 'group' ],\n  data() {\n      return {\n        visibleDelete: false,\n        visibleCommand: false,\n        btnName: '',\n        groupName: '',\n        dialog: false,\n        commands: [],\n        ip: '',\n        params: '',\n      }\n  },\n  async created(){\n    this.getCommand();\n  },\n  methods: {\n    async getCommand(){\n      let service = this.service\n      this.commands = (await novoComando.getOneCommand(service.chipset, service.mac, service.number)).data[0]['command']\n      console.log(\"Todos os comandos são: \", this.commands)\n    },\n    deleteService(){\n      novoGrupo.deleteInGroup(this.group, this.service)\n    },\n    createCommand(){\n      let comando = { \n        \"ip\": this.ip,\n        \"params\": this.params,\n        \"name\": this.btnName\n      }\n      fetch(novoComando.addCommand(this.service, comando))\n      .then(this.getCommand())\n      \n    },\n    deleteCommand(service, command){\n      novoComando.deleteCommand(service, command)\n      this.getCommand()\n    },\n    editService(){\n      novoComando.changeCommand(this.service)\n    }\n  }\n}\n</script>\n\n<style scoped>\n.toCenter{\n  text-align: center;\n}\ntable {\n  font-family: arial, sans-serif;\n  border-collapse: collapse;\n  width: 100%;\n}\n\ntd, th {\n  border: 1px solid #dddddd;\n  text-align: left;\n  padding: 8px;\n}\n\ntr:nth-child(even) {\n  background-color: #dddddd;\n}\n</style>"],"sourceRoot":"src/components/service"}]}