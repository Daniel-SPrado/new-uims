AA,CAAA,IAAA;AACA,eAFA,EAEA,KAFA,CAAA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,GA7BA;AA8BA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,oBACA;AACA,WAAA,WAAA,GAAA,CAAA,KAAA,WAAA;AACA,KAHA;AAIA,IAAA,IAJA,kBAIA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,SAAA,GAAA,KAAA;AACA,gBAAA,MAAA,CAAA,WAAA,CAAA,OAAA,GAAA,SAAA;AACA,gBAAA,MAAA,CAAA,WAAA,CAAA,aAAA,GAAA,MAAA,CAAA,UAAA,CAAA,IAAA;AAHA;AAAA,uBAIA,KAAA,CAAA,IAAA,CAAA,MAAA,CAAA,MAAA,CAAA,EAAA,CAJA;;AAAA;AAIA,gBAAA,MAJA,kBAIA,IAJA;AAKA,gBAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA,gBAAA,MAAA,CAAA,WAAA,CAAA,OAAA,GAAA,MAAA,CAAA,QAAA;;AACA,oBAAA,MAAA,IAAA,IAAA,EAAA;AACA,kBAAA,MAAA,CAAA,WAAA,CAAA,OAAA,GAAA,IAAA;AACA,kBAAA,MAAA,CAAA,WAAA,CAAA,aAAA,GAAA,MAAA,CAAA,UAAA,CAAA,OAAA;AACA,kBAAA,MAAA,CAAA,SAAA,GAAA,IAAA;AACA;;AAAA,oBAAA,MAAA,IAAA,MAAA,EAAA;AACA,kBAAA,MAAA,CAAA,WAAA,CAAA,OAAA,GAAA,MAAA;AACA,kBAAA,MAAA,CAAA,WAAA,CAAA,aAAA,GAAA,MAAA,CAAA,UAAA,CAAA,WAAA;AACA,kBAAA,MAAA,CAAA,SAAA,GAAA,KAAA;AACA;;AAfA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBA;AApBA;AA9BA,CAAA","sourcesContent":["<template>\n    <div>\n        <v-card class=\"card\">\n            <span>\n                <div style=\"width: 100%; height: 5px;\" \n                    v-bind:style=\"{backgroundColor: client_info.network_color}\" />\n            </span>\n            <v-card-title>\n                <div class=\"primary--text text-xs-center ma-3\"> \n                    {{ client.name }} \n                </div>\n            </v-card-title>\n            <v-card-text v-show=\"isShow\" class=\"text-xs-center \">\n                <div class=\"card-information\"> \n                    <span class=\"ma-3\"> Status:  <span v-bind:style=\"{color: client_info.status_color}\"> {{ client_info.status }}  </span> </span><br>\n                    <span class=\"ma-3\"> Network: <span :class=\"{ \n                        'on': client_info.network == 'On',\n                        'fail': client_info.network == 'Fail',\n                        'try': client_info.network == 'Ping...'\n                        }\"> {{ client_info.network }} </span> </span>\n                </div>\n            </v-card-text>\n            <v-card-text v-show=\"!isShow\" class=\"text-xs-center \">\n                <div class=\"card-content\" @click=\"alerta()\">\n                    <span class=\"ma-3\"> channel:  {{ client.channel }} </span><br>\n                    <span class=\"ma-3\"> chipset:  {{ client.chipset }} </span><br>\n                    <span class=\"ma-3\"> processor:{{ client.processor }} </span><br>\n                    <span class=\"ma-3\"> IP: {{ client.ip }} </span>\n                </div>\n            </v-card-text>\n            <v-card-text>\n                <div class=\"\"> \n                    \n                </div>\n            </v-card-text>\n            <v-divider></v-divider>\n            <v-card-actions v-show=\"!show_delete\"> \n                <Service v-bind:client=\"client\"></Service>\n                <!-- <v-btn text disabled v-show=\"!isWorking\"> Searching.. </v-btn> -->\n                <v-spacer></v-spacer>\n                <v-btn class=\"primary--text\" text @click=\"isShow = !isShow\"> Config </v-btn>\n            </v-card-actions>\n            <v-card-actions class=\"red\" v-show=\"show_delete\">\n                <v-btn class=\"delete\" text> Delete </v-btn>\n            </v-card-actions>\n        </v-card>\n\n    </div> \n</template>\n\n<script>\nimport Service from \"./Service.vue\";\nimport { Network } from \"../function/index.js\";\n// import Ping from 'ping.js'\n\nlet ping = new Network();  \n\nexport default {    \n    props: [ 'client' ],\n    components: {\n        Service\n    },\n    data(){\n        return {\n            isShow: true,\n            isWorking: false,\n            show_delete: false,\n            client_info: {\n                network_color: \"#d81818\",\n                status_color: '',\n                status: 'Unreacheable',\n                network: 'Fail'\n            },\n            color_info: {\n                working: \"#33b113\",\n                ping: \"#d88e18\",\n                not_working: \"#d81818\"\n            },\n        }    \n    },\n    async created(){\n    },\n    async mounted(){\n        setInterval( () => {\n            this.ping()\n        }, 30000 );\n    },\n    methods: {\n        alerta(){\n            this.show_delete = !this.show_delete\n        },\n        async ping() {\n            this.isWorking = false\n            this.client_info.network = \"Ping...\"\n            this.client_info.network_color = this.color_info.ping\n            let result = (await ping.PING(this.client.ip)).data\n            console.log(result)\n            this.client_info.network = result.response\n            if(result == 'On'){\n                this.client_info.network = \"On\"\n                this.client_info.network_color = this.color_info.working\n                this.isWorking = true\n            }if(result == 'Fail'){\n                this.client_info.network = \"Fail\"\n                this.client_info.network_color = this.color_info.not_working\n                this.isWorking = false\n            }\n        },\n    }\n}\n</script>\n\n<style scoped>\n.toCenter{\n    align-content: center;\n    text-align: center;\n}\n.main_title{\n    font-size: 21px;\n    color: rgb(80, 80, 80);\n}\n.exampleColor{\n    color: #e91414da;\n}\n\n.card-information{\n    line-height: 32px;\n    font-size: 16px;\n    font-family: 'Electrolize', sans-serif;\n    border-radius: 5px;\n    background-color: rgba(175, 180, 206, 0.246);\n    margin-bottom: -20px;\n}\n\n.card-status{\n    color: #d86e18;\n}\n\n.card-content{\n    width: 100%;\n    line-height: 32px;\n    font-size: 16px;\n    font-family: 'Electrolize', sans-serif;\n    border-radius: 5px;\n    background-color: rgba(211, 209, 118, 0.692);\n    margin-top: -20px;\n    margin-bottom: -20px;\n}\n\n.on{\n    color: green;\n}\n\n.try{\n    color: #d89e18;\n}\n\n.fail{\n    color: #d81818;\n}\n</style>"],"sourceRoot":"src/components"}]}