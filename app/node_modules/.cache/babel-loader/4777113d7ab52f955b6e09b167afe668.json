AAA,KAAA,CAAA,OADA,iBACA,IADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,GAzBA;AA0BA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,uBACA,MADA,EACA;AACA,WAAA,UAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,MAAA,CAAA;AACA,WAAA,SAAA,GAAA,IAAA;AACA,KAJA;AAKA,IAAA,WALA,uBAKA,MALA,EAKA;AACA,MAAA,KAAA,CAAA,UAAA,CAAA,KAAA,KAAA,CAAA,GAAA,EAAA,MAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,KAAA,CAAA,GAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA;AATA;AA1BA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-dialog\n      v-model=\"dialog\"\n      width=\"80%\"\n    >\n      <template v-slot:activator=\"{ on }\">\n        <v-btn\n          text\n          v-on=\"on\"\n        >\n          <v-icon>mdi-format-list-bulleted</v-icon>\n        </v-btn>\n      </template>\n      <v-card>\n        <v-card-title\n          class=\"headline green lighten-2\"\n          primary-title\n        >\n          All available clients\n          <v-spacer></v-spacer>\n          {{ group.group }}\n        </v-card-title>\n        <br>\n        <v-card-text>    \n          <table>\n            <tr>\n                <th>Name</th>\n                <th>Channel</th>\n                <th>Mac</th>\n                <th>Chipset</th>\n                <th>Add</th>\n            </tr>\n            <tr v-for=\"item in clients\" :key=\"item.mac\"> \n                <td class=\"toCenter\">{{item.name}}</td>\n                <td class=\"toCenter\">{{item.channel}}</td>\n                <td class=\"toCenter\">{{item.mac}}</td>\n                <td class=\"toCenter\">{{item.chipset}}</td>\n                <td> <v-btn text @click=\"bind_client(item)\"> <v-icon>mdi-check</v-icon> </v-btn> </td> \n            </tr>\n          </table>\n          <v-dialog\n            width=\"10%\"\n            v-model=\"ip_dialog\"\n          >\n            <v-card>\n              <v-card-title class=\"headline grey lighten-2\"> \n                Add IP               \n              </v-card-title><br>\n              <v-card-text>\n                <v-text-field solo label=\"IP\" v-model=\"new_client.ip\"></v-text-field>\n              </v-card-text>\n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn text color=\"green\" @click=\"send_client(new_client)\">\n                  Create\n                </v-btn>\n              </v-card-actions>\n            </v-card>\n          </v-dialog>\n        </v-card-text>\n        <v-divider></v-divider>\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn\n            color=\"red\"\n            text\n            @click=\"dialog = false\"\n          >\n            Close\n          </v-btn>\n\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </div>\n</template>\n\n<script>\nimport { Client, Group } from \"../../function/index.js\";\n\nlet client = new Client();\nlet group = new Group();\n\nexport default {\n  props: [ \"group\" ],\n  components: {  },\n  data() {\n      return {\n        selectClient: true,\n        clients: null,\n        dialog: false,\n        ip_dialog: false,\n        new_client: {\n          channel: '',\n          chipset: '',\n          location: '',\n          mac: '',\n          name: '',\n          processor: '',\n          serial: '',\n          time: '',\n          ip: ''\n        }\n\n      }\n  },\n  async mounted(){\n    this.clients = (await client.GET_CLIENT()).data\n  },\n  methods: {\n    bind_client(client){\n      this.new_client = Object.assign({}, client)\n      this.ip_dialog = true\n    },\n    send_client(client){\n      group.ADD_CLIENT(this.group._id, client)\n      console.log(this.group._id)\n      console.log(client)\n    }\n  }\n}\n</script>\n\n<style scoped>\ntable {\n  font-family: arial, sans-serif;\n  border-collapse: collapse;\n  width: 100%;\n}\n\ntd, th {\n  border: 1px solid #dddddd;\n  text-align: left;\n  padding: 8px;\n}\n\ntr:nth-child(even) {\n  background-color: #acacac;\n}\n\n</style>"],"sourceRoot":"src/components/client"}]}